<%= form_for(@favlist) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <div class="field">
    <%= f.text_area :title, placeholder: "Title..." %>
    <%= f.text_area :memo, placeholder: "Memo..." %>
  </div>
  <%= f.submit "お気に入りリストを作成する", class: "btn btn-primary" %>
<% end %>
