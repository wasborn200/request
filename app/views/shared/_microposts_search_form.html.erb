<%= search_form_for @q, url: @url do |f| %>
  <%= f.label :content_cont, 'キーワード検索' %>
  <div class="input-group">
    <%= f.text_field :content_cont, placeholder: "Enter keyword...",
                          class: 'form-control' %>
    <span class="input-group-btn">
      <%= f.submit '検索', class: "btn btn-primary" %>
    </span>
  </div>
<% end %>
